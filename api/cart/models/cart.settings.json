{
  "kind": "collectionType",
  "collectionName": "carts",
  "info": {
    "name": "Cart"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "product": {
      "model": "product",
      "via": "carts"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "carts"
    }
  }
}
